{
	"name": "Historical_Weather_source",
	"properties": {
		"parameters": {
			"BaseURL": {
				"type": "string",
				"defaultValue": "https://history.openweathermap.org/"
			},
			"Pathsuffix": {
				"type": "string",
				"defaultValue": "@concat(   'data/2.5/history/city?q=Paris,FR&type=hour&start=',   string(div(ticks(startOfMonth(addMonths(utcNow(), -1))), 10000000)),   '&end=',   string(div(ticks(addSeconds(addDays(startOfMonth(utcNow()), -1), 86399)), 10000000)),   '&appid=YOUR_API_KEY' )"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "@{linkedService().BaseURL}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}